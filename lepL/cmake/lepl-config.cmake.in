# lepL CMake config file
#
# This file sets the following variables:
# lepL_FOUND - Always TRUE.
# lepL_INCLUDE_DIRS - Directories containing the lepL include files.
# lepL_IDL_DIRS - Directories containing the lepL IDL files.
# lepL_LIBRARIES - Libraries needed to use lepL.
# lepL_DEFINITIONS - Compiler flags for lepL.
# lepL_VERSION - The version of lepL found.
# lepL_VERSION_MAJOR - The major version of lepL found.
# lepL_VERSION_MINOR - The minor version of lepL found.
# lepL_VERSION_REVISION - The revision version of lepL found.
# lepL_VERSION_CANDIDATE - The candidate version of lepL found.

message(STATUS "Found lepL-@PROJECT_VERSION@")
set(lepL_FOUND TRUE)

find_package(<dependency> REQUIRED)

#set(lepL_INCLUDE_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@"
#    ${<dependency>_INCLUDE_DIRS}
#    )
#
#set(lepL_IDL_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@/idl")
set(lepL_INCLUDE_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@"
    ${<dependency>_INCLUDE_DIRS}
    )
set(lepL_IDL_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@/idl")


if(WIN32)
    set(lepL_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_STATIC_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
else(WIN32)
    set(lepL_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_SHARED_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
endif(WIN32)

set(lepL_DEFINITIONS ${<dependency>_DEFINITIONS})

set(lepL_VERSION @PROJECT_VERSION@)
set(lepL_VERSION_MAJOR @PROJECT_VERSION_MAJOR@)
set(lepL_VERSION_MINOR @PROJECT_VERSION_MINOR@)
set(lepL_VERSION_REVISION @PROJECT_VERSION_REVISION@)
set(lepL_VERSION_CANDIDATE @PROJECT_VERSION_CANDIDATE@)

